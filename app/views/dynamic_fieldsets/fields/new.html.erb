<h1>New field</h1>

<%= form_for(@field) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>

<% if params[:parent].nil? %>
  <%= link_to 'Back', dynamic_fieldsets_fields_path %>
<% else %>
  <%= link_to 'Back', dynamic_fieldsets_children_dynamic_fieldsets_fieldset_path(params[:parent]) %>
<% end %>
