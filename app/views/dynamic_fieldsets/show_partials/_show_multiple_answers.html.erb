<div class='dynamic_fieldsets field'>
  <span class='label'><%= label %></label>
  <% if values.nil? || values.empty? %>
    <em class='empty'>No answer given</em>
  <% else %>
    <ul>
      <% values.each do |value| %>
        <li>
          <span class='value'><%= value %></span>
        </li>
      <% end %>
    </ul>
  <% end %>
</div>

